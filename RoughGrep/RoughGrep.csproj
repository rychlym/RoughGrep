<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <OutputType>WinExe</OutputType>
    <AssemblyName>rgg</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>
  <PropertyGroup>
    <StartupObject>RoughGrep.Program</StartupObject>
  </PropertyGroup>
  <PropertyGroup>
    <ApplicationIcon>roughgrep.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <Compile Update="SearchControl.cs">
      <SubType>UserControl</SubType>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <Content Include="roughgrep.ico" />
  </ItemGroup>
    <ItemGroup>
    <Reference Include="System.Windows.Forms" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="jacobslusser.ScintillaNET" Version="3.6.3" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="schemy" Version="1.0.0" />
    <PackageReference Include="TrivialBehinds" Version="1.2.0" />
  </ItemGroup>
</Project>